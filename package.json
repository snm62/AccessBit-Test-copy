{
  "name": "Accessibility",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm install && concurrently \"webflow extension serve\" \"npm run watch-webpack\"",
    "dev:localhost": "webpack serve --config webpack.config.mjs --mode development",
    "build": "npm run build-webpack && webflow extension bundle",
    "watch-webpack": "webpack --config webpack.config.mjs --mode development --watch",
    "build-webpack": "webpack --config webpack.prod.config.mjs",
    "lint": "eslint . --ext .ts,.tsx",
    "worker:dev": "wrangler dev",
    "worker:deploy": "wrangler deploy",
    "worker:deploy:staging": "wrangler deploy --env staging",
    "worker:deploy:prod": "wrangler deploy --env production",
    "kv:create": "wrangler kv namespace create accessbit-test-kv",
    "kv:list": "wrangler kv namespace list"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@webflow/designer-extension-typings": "^2.0.2",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.5.7",
    "eslint-plugin-promise": "^6.1.1",
    "mkcert": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@jaames/iro": "^5.5.2",
    "@tanstack/react-query": "^5.87.4",
    "jwt-decode": "^4.0.0",
    "react-dom": "^18.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
